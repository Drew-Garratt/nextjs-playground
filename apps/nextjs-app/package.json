{
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build-fast": "cross-env NEXT_BUILD_ENV_SENTRY_ENABLED=0 NEXT_BUILD_ENV_SOURCEMAPS=0 NEXT_BUILD_ENV_TYPECHECK=0 NEXT_BUILD_ENV_LINT=0 next build",
    "bundle-analyze": "cross-env ANALYZE=true NEXT_BUILD_ENV_SENTRY_ENABLED=1 NEXT_BUILD_ENV_TYPECHECK=0 NEXT_BUILD_ENV_LINT=0 pnpm build",
    "clean": "rimraf ./.next ./.out ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "dev": "next dev --turbo",
    "dev-msw": "cross-env NODE_NO_WARNINGS=1 MOCKING=true next dev --turbo",
    "fix-all-files": "eslint --fix",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "lint": "eslint --cache --cache-location ../../.cache/eslint/nextjs-app.eslintcache",
    "start": "next start",
    "test": "pnpm test-unit",
    "test-e2e": "cross-env E2E_WEBSERVER_MODE=BUILD_AND_START playwright test",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest watch --ui",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@acme/ui": "workspace:*",
    "@sentry/nextjs": "8",
    "@t3-oss/env-nextjs": "0.11.0",
    "babel-plugin-react-compiler": "0.0.0-experimental-696af53-20240625",
    "cross-env": "7.0.3",
    "next": "catalog:next15",
    "next-intl": "3.17.2",
    "picocolors": "1.0.1",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "zod": "catalog:",
    "classnames": "2.5.1",
    "next-themes": "0.3.0"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@chromatic-com/storybook": "^1.6.1",
    "@faker-js/faker": "8.4.1",
    "@next/bundle-analyzer": "14.2.5",
    "@next/env": "14.2.5",
    "@playwright/test": "1.45.3",
    "@size-limit/file": "11.1.4",
    "@storybook/addon-essentials": "8.2.6",
    "@storybook/addon-interactions": "8.2.6",
    "@storybook/addon-links": "8.2.6",
    "@storybook/addon-onboarding": "8.2.6",
    "@storybook/addon-themes": "8.2.6",
    "@storybook/blocks": "8.2.6",
    "@storybook/nextjs": "8.2.6",
    "@storybook/react": "8.2.6",
    "@storybook/test": "8.2.6",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.13",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.14.12",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vitejs/plugin-react": "4.3.1",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/browser": "2.0.4",
    "@vitest/coverage-v8": "2.0.4",
    "@vitest/ui": "2.0.4",
    "autoprefixer": "10.4.19",
    "cross-env": "7.0.3",
    "dotenv-cli": "7.4.2",
    "es-check": "7.2.1",
    "eslint": "catalog:",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "24.1.1",
    "msw": "2.3.4",
    "playwright-msw": "3.0.1",
    "prettier": "catalog:",
    "rimraf": "6.0.1",
    "storybook": "^8.2.6",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "5.3.5",
    "vite-plugin-magical-svg": "1.3.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.4"
  },
  "prettier": "@acme/prettier-config",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
